{
  "name": "anteact",
  "version": "0.1.0",
  "description": "SSP Admin Control Panel",
  "engines": {
    "npm": ">=6"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "dev": "babel-node tools/devServer.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild1": "npm run clean-dist && npm run lint && npm run test",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "build:dll": "webpack --config build/webpack.dll.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "build:stats": "webpack --profile --json > stats.json"
  },
  "author": "nnecec",
  "license": "MIT",
  "dependencies": {
    "antd": "2.9.1",
    "axios": "0.16.1",
    "object-assign": "4.1.1",
    "randomcolor": "0.4.4",
    "react": "15.4.2",
    "react-cookie": "1.0.5",
    "react-dom": "15.4.2",
    "react-redux": "5.0.4",
    "react-redux-loading-bar": "2.8.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.8",
    "recharts": "0.22.1",
    "redux": "3.6.0",
    "redux-observable": "0.14.1",
    "redux-saga": "0.14.6",
    "redux-thunk": "2.2.0",
    "rxjs": "5.3.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-import": "1.1.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.8",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "0.4.0",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-watch": "3.1.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "0.11.1",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "19.0.2",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "markdown-loader": "2.0.0",
    "mockdate": "2.0.1",
    "mockjs": "1.0.1-beta3",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "open": "0.0.5",
    "postcss-loader": "1.3.3",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "2.0.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-bundle-analyzer": "2.4.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    }
  }
}
